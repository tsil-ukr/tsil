секція КЦ {
  місцева дія обʼєкт_сирого_значення_як(компілятор: адреса<Компілятор>, контекст: Контекст, місцезнаходження: розбирач::Місцезнаходження, обʼєкт_сирого_значення: адреса<ОбʼєктСирогоЗначення>, аргумент: Аргумент): РезультатОбʼєкта {
    якщо аргумент.обʼєкт.вид == ВидОбʼєктаТип {
      змінна тип = аргумент.обʼєкт.дані як адреса<ОбʼєктТипу>;

      якщо обʼєкт_сирого_значення.вид == ВидСирогоОбʼєктаПусто {
        змінна результат = створити_пустий_обʼєкт(компілятор, контекст, місцезнаходження, тип);

        якщо результат.помилка != пусто {
          вернути РезультатОбʼєкта { помилка = результат.помилка, ... };
        }

        вернути РезультатОбʼєкта {
          Обʼєкт {
            ВидОбʼєктаЗначення,
            результат.обʼєкт_значення
          },
          помилка = пусто
        };
      }

      якщо обʼєкт_сирого_значення.вид == ВидСирогоОбʼєктаЧисло {
        змінна результат_розбору_числа = розібрати_число(
          компілятор,
          контекст,
          місцезнаходження,
          обʼєкт_сирого_значення.дані,
          тип
        );

        якщо результат_розбору_числа.помилка != пусто {
          вернути РезультатОбʼєкта { ..., помилка = результат_розбору_числа.помилка };
        }

        вернути РезультатОбʼєкта {
          Обʼєкт {
            ВидОбʼєктаЗначення,
            результат_розбору_числа.обʼєкт_значення
          },
          помилка = пусто
        };
      }
    }

    вернути результат_обʼєкта_помилка(компілятор, контекст.розібраний_текст, місцезнаходження, "Не вдалось скомпілювати");
  }
}

