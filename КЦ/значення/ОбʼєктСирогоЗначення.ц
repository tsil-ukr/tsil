взяти визначення КЦ;

секція КЦ {
  місцева дія виділити_обʼєкт_сирого_значення(компілятор: адреса<Компілятор>): адреса<ОбʼєктСирогоЗначення> {
    змінна обʼєкт_сирого_значення = виділити<ОбʼєктСирогоЗначення>(компілятор);

    обʼєкт_сирого_значення.наступний = пусто;
    обʼєкт_сирого_значення.компілятор = компілятор;
    обʼєкт_сирого_значення.вид = ВидСирогоОбʼєктаЧисло;
    обʼєкт_сирого_значення.дані = т8 { 0, пусто };

    якщо компілятор.перший_обʼєкт_сирого_значення == пусто {
      компілятор.перший_обʼєкт_сирого_значення = обʼєкт_сирого_значення;
    }
    якщо компілятор.останній_обʼєкт_сирого_значення == пусто {
      компілятор.останній_обʼєкт_сирого_значення = обʼєкт_сирого_значення;
    } інакше {
      компілятор.останній_обʼєкт_сирого_значення.наступний = обʼєкт_сирого_значення;
      компілятор.останній_обʼєкт_сирого_значення = обʼєкт_сирого_значення;
    }

    вернути обʼєкт_сирого_значення;
  }

  місцева дія знищити_обʼєкт_сирого_значення(компілятор: адреса<Компілятор>, обʼєкт_сирого_значення: адреса<ОбʼєктСирогоЗначення>) {
    звільнити_памʼять(компілятор, обʼєкт_сирого_значення.дані.дані);

    звільнити(компілятор, обʼєкт_сирого_значення);
  }
}

