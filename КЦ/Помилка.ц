взяти визначення Ц;

секція ц {
  дія видимість_в_ю8(видимість: позитивне): ю8 {
    якщо видимість == ВидимістьДіїЗовнішня {
      вернути ю8"зовнішня";
    } інакше якщо видимість == ВидимістьДіїМісцева {
      вернути ю8"місцева";
    } інакше {
      вернути ю8"внутрішня";
    }
  }

  місцева дія обʼєкт_типу_як_ю8сі(обʼєкт_типу: адреса<ОбʼєктТипу>): ю8 {
    якщо обʼєкт_типу.вид == ВидТипуНативний {
      якщо обʼєкт_типу.назва == пусто {
        вернути ю8"<тип без назви>";
      }
      вернути обʼєкт_типу.назва.значення;
    }
    якщо обʼєкт_типу.вид == ВидТипуСтруктура {
      якщо обʼєкт_типу.назва == пусто {
        вернути ю8"<структура без назви>";
      }
      вернути обʼєкт_типу.назва.значення;
    }
    якщо обʼєкт_типу.вид == ВидТипуДія {
      ціль буфер_ю8 = створити_буфер_ю8();
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8"(");
      змінна ціль пх: позитивне = 0;
      поки пх < обʼєкт_типу.параметри_дії.розмір {
        додати_ю8_до_буфера_ю8(буфер_ю8, обʼєкт_типу_як_ю8сі(обʼєкт_типу.параметри_дії.дані[пх].тип));
        якщо пх != обʼєкт_типу.параметри_дії.розмір - 1 {
          додати_ю8_до_буфера_ю8(буфер_ю8, ю8", ");
        }
        пх = пх + 1;
      }
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8") -> ");
      додати_ю8_до_буфера_ю8(буфер_ю8, обʼєкт_типу_як_ю8сі(обʼєкт_типу.тип_результату_дії));
      ціль результат = створити_ю8_з_буфера(буфер_ю8);
      звільнити_буфер_ю8(буфер_ю8);
      вернути результат;
    }
    якщо обʼєкт_типу.вид == ВидТипуАдреса {
      ціль буфер_ю8 = створити_буфер_ю8();
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8"адреса<");
      додати_ю8_до_буфера_ю8(буфер_ю8, обʼєкт_типу_як_ю8сі(обʼєкт_типу.тип_адреси));
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8">");
      ціль результат = створити_ю8_з_буфера(буфер_ю8);
      звільнити_буфер_ю8(буфер_ю8);
      вернути результат;
    }
    якщо обʼєкт_типу.вид == ВидТипуПамʼять {
      ціль буфер_ю8 = створити_буфер_ю8();
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8"памʼять<");
      додати_ю8_до_буфера_ю8(буфер_ю8, обʼєкт_типу_як_ю8сі(обʼєкт_типу.тип_памʼяті));
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8">");
      ціль результат = створити_ю8_з_буфера(буфер_ю8);
      звільнити_буфер_ю8(буфер_ю8);
      вернути результат;
    }
    якщо обʼєкт_типу.вид == ВидТипуМасив {
      ціль буфер_ю8 = створити_буфер_ю8();
      додати_ю8_до_буфера_ю8(буфер_ю8, обʼєкт_типу_як_ю8сі(обʼєкт_типу.тип_масиву));
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8"[");
//      додати_до_буфера_ю8(буфер_ю8, розмір);
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8"]");
      ціль результат = створити_ю8_з_буфера(буфер_ю8);
      звільнити_буфер_ю8(буфер_ю8);
      вернути результат;
    }
    якщо обʼєкт_типу.вид == ВидТипуВаріація {
      ціль буфер_ю8 = створити_буфер_ю8();
      змінна ціль пх: позитивне = 0;
      поки пх < обʼєкт_типу.типи_варіації.розмір {
        додати_ю8_до_буфера_ю8(буфер_ю8, обʼєкт_типу_як_ю8сі(обʼєкт_типу.типи_варіації.дані[пх]));
        якщо пх != обʼєкт_типу.типи_варіації.розмір - 1 {
          додати_ю8_до_буфера_ю8(буфер_ю8, ю8" | ");
        }
        пх = пх + 1;
      }
      ціль результат = створити_ю8_з_буфера(буфер_ю8);
      звільнити_буфер_ю8(буфер_ю8);
      вернути результат;
    }
    якщо обʼєкт_типу.вид == ВидТипуПерелік {
      якщо обʼєкт_типу.назва == пусто {
        вернути ю8"<тип без назви>";
      }
      вернути обʼєкт_типу.назва.значення;
    }
    якщо обʼєкт_типу.вид == ВидТипуЕлементПереліку {
      якщо обʼєкт_типу.назва == пусто {
        вернути ю8"<тип без назви>";
      }
      вернути обʼєкт_типу.назва.значення;
    }
    вернути ю8"<тип без назви>";
  }

  місцева дія обʼєкт_як_ю8сі(обʼєкт: Обʼєкт): ю8 {
    якщо обʼєкт.вид == ВидОбʼєктаТип {
      ціль буфер_ю8 = створити_буфер_ю8();
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8"<тип ");
      додати_ю8_до_буфера_ю8(буфер_ю8, обʼєкт_типу_як_ю8сі(обʼєкт.дані як адреса<ОбʼєктТипу>));
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8">");
      ціль результат = створити_ю8_з_буфера(буфер_ю8);
      звільнити_буфер_ю8(буфер_ю8);
      вернути результат;
    }
    якщо обʼєкт.вид == ВидОбʼєктаДія {
      ціль буфер_ю8 = створити_буфер_ю8();
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8"<дія>");
      ціль результат = створити_ю8_з_буфера(буфер_ю8);
      звільнити_буфер_ю8(буфер_ю8);
      вернути результат;
    }
    якщо обʼєкт.вид == ВидОбʼєктаГрупаДій {
      ціль буфер_ю8 = створити_буфер_ю8();
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8"<група дій>");
      ціль результат = створити_ю8_з_буфера(буфер_ю8);
      звільнити_буфер_ю8(буфер_ю8);
      вернути результат;
    }
    якщо обʼєкт.вид == ВидОбʼєктаСекція {
      ціль буфер_ю8 = створити_буфер_ю8();
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8"<секція>");
      ціль результат = створити_ю8_з_буфера(буфер_ю8);
      звільнити_буфер_ю8(буфер_ю8);
      вернути результат;
    }
    якщо обʼєкт.вид == ВидОбʼєктаШаблон {
      ціль буфер_ю8 = створити_буфер_ю8();
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8"<шаблон>");
      ціль результат = створити_ю8_з_буфера(буфер_ю8);
      звільнити_буфер_ю8(буфер_ю8);
      вернути результат;
    }
    якщо обʼєкт.вид == ВидОбʼєктаСиреЗначення {
      ціль буфер_ю8 = створити_буфер_ю8();
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8"<сире значення>");
      ціль результат = створити_ю8_з_буфера(буфер_ю8);
      звільнити_буфер_ю8(буфер_ю8);
      вернути результат;
    }
    якщо обʼєкт.вид == ВидОбʼєктаНезавантаженеЗначення {
      ціль буфер_ю8 = створити_буфер_ю8();
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8"<незавантажене значення ");
      додати_ю8_до_буфера_ю8(буфер_ю8, обʼєкт_типу_як_ю8сі((обʼєкт.дані як ОбʼєктНезавантаженогоЗначення).тип));
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8">");
      ціль результат = створити_ю8_з_буфера(буфер_ю8);
      звільнити_буфер_ю8(буфер_ю8);
      вернути результат;
    }
    якщо обʼєкт.вид == ВидОбʼєктаЗначення {
      ціль буфер_ю8 = створити_буфер_ю8();
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8"<значення ");
      додати_ю8_до_буфера_ю8(буфер_ю8, обʼєкт_типу_як_ю8сі((обʼєкт.дані як ОбʼєктЗначення).тип));
      додати_ю8_до_буфера_ю8(буфер_ю8, ю8">");
      ціль результат = створити_ю8_з_буфера(буфер_ю8);
      звільнити_буфер_ю8(буфер_ю8);
      вернути результат;
    }
    вернути ю8"<невідомий обʼєкт>";
  }

  місцева дія виділити_помилку_компіляції(місцезнаходження: адреса<Місцезнаходження>, повідомлення: ю8): адреса<ПомилкаКомпіляції> {
    ціль помилка = виділити<ПомилкаКомпіляції>();
    помилка.місцезнаходження = місцезнаходження;
    помилка.повідомлення = повідомлення;
    вернути помилка;
  }

  місцева дія виділити_помилку_компіляції_субʼєкт_вже_визначено(місцезнаходження: адреса<Місцезнаходження>, назва: ю8): адреса<ПомилкаКомпіляції> {
    ціль помилка_компіляції = виділити<ПомилкаКомпіляції>();
    помилка_компіляції.місцезнаходження = місцезнаходження;
    ціль буфер_повідомлення = створити_буфер_ю8();
    додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8"Субʼєкт \"");
    додати_ю8_до_буфера_ю8(буфер_повідомлення, назва);
    додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8"\" вже визначено");
    помилка_компіляції.повідомлення = створити_ю8_з_буфера(буфер_повідомлення);
    звільнити_буфер_ю8(буфер_повідомлення);
    вернути помилка_компіляції;
  }

  місцева дія виділити_помилку_компіляції_субʼєкт_не_визначено(місцезнаходження: адреса<Місцезнаходження>, назва: ю8): адреса<ПомилкаКомпіляції> {
    ціль помилка_компіляції = виділити<ПомилкаКомпіляції>();
    помилка_компіляції.місцезнаходження = місцезнаходження;
    ціль буфер_повідомлення = створити_буфер_ю8();
    додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8"Субʼєкт \"");
    додати_ю8_до_буфера_ю8(буфер_повідомлення, назва);
    додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8"\" не визначено");
    помилка_компіляції.повідомлення = створити_ю8_з_буфера(буфер_повідомлення);
    звільнити_буфер_ю8(буфер_повідомлення);
    вернути помилка_компіляції;
  }

  місцева дія виділити_помилку_компіляції_неможливо_виконати(місцезнаходження: адреса<Місцезнаходження>, обʼєкт: Обʼєкт): адреса<ПомилкаКомпіляції> {
    ціль помилка_компіляції = виділити<ПомилкаКомпіляції>();
    помилка_компіляції.місцезнаходження = місцезнаходження;
    ціль буфер_повідомлення = створити_буфер_ю8();
    додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8"Неможливо виконати");
    якщо обʼєкт.вид == ВидОбʼєктаТип {
      додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8" структуру");
    } інакше якщо обʼєкт.вид == ВидОбʼєктаДія {
      додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8" дію");
    } інакше якщо обʼєкт.вид == ВидОбʼєктаЗначення {
      додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8" \"");
      ціль обʼєкт_значення = обʼєкт.дані як ОбʼєктЗначення;
      додати_ю8_до_буфера_ю8(буфер_повідомлення, обʼєкт_типу_як_ю8сі(обʼєкт_значення.тип));
      додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8"\"");
    } інакше якщо обʼєкт.вид == ВидОбʼєктаСекція {
      додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8" секцію");
    } інакше якщо обʼєкт.вид == ВидОбʼєктаНезавантаженеЗначення {
      додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8" \"");
      ціль обʼєкт_незавантаженого_значення = обʼєкт.дані як ОбʼєктНезавантаженогоЗначення;
      додати_ю8_до_буфера_ю8(буфер_повідомлення, обʼєкт_типу_як_ю8сі(обʼєкт_незавантаженого_значення.тип));
      додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8"\"");
    } інакше якщо обʼєкт.вид == ВидОбʼєктаШаблон {
      додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8" шаблон");
    } інакше {
      додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8" невідоме значення (вітання, ви натрапили на помилку компілятора)");
    }
    помилка_компіляції.повідомлення = створити_ю8_з_буфера(буфер_повідомлення);
    звільнити_буфер_ю8(буфер_повідомлення);
    вернути помилка_компіляції;
  }

  місцева дія виділити_помилку_компіляції_не_вдалось_знайти_відповідну_дію(місцезнаходження: адреса<Місцезнаходження>): адреса<ПомилкаКомпіляції> {
    ціль помилка_компіляції = виділити<ПомилкаКомпіляції>();
    помилка_компіляції.місцезнаходження = місцезнаходження;
    ціль буфер_повідомлення = створити_буфер_ю8();
    додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8"Не вдалося знайти відповідну дію");
    помилка_компіляції.повідомлення = створити_ю8_з_буфера(буфер_повідомлення);
    звільнити_буфер_ю8(буфер_повідомлення);
    вернути помилка_компіляції;
  }

  місцева дія виділити_помилку_компіляції_неможливо_виконати_шаблон(місцезнаходження: адреса<Місцезнаходження>, обʼєкт: Обʼєкт): адреса<ПомилкаКомпіляції> {
