    помилка_компіляції.повідомлення = створити_ю8_сі_з_буфера(буфер_повідомлення);
    звільнити_буфер_ю8(буфер_повідомлення);
    вернути помилка_компіляції;
  }

  місцева дія виділити_помилку_компіляції_тип_не_є_повним(місцезнаходження: комірка<Місцезнаходження>, обʼєкт_типу: комірка<ОбʼєктТипу>): комірка<ПомилкаКомпіляції> {
    ціль помилка_компіляції = виділити<ПомилкаКомпіляції>();
    помилка_компіляції.місцезнаходження = місцезнаходження;
    ціль буфер_повідомлення = створити_буфер_ю8();
    додати_до_буфера_ю8(буфер_повідомлення, ю8"Тип \"");
    додати_до_буфера_ю8(буфер_повідомлення, обʼєкт_типу_як_ю8сі(обʼєкт_типу));
    додати_до_буфера_ю8(буфер_повідомлення, ю8"\" не є повним");
    помилка_компіляції.повідомлення = створити_ю8_сі_з_буфера(буфер_повідомлення);
    звільнити_буфер_ю8(буфер_повідомлення);
    вернути помилка_компіляції;
  }
}