    ціль буфер_повідомлення = створити_буфер_ю8();
    додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8"Невірний тип значення цілі \"");
    додати_ю8_до_буфера_ю8(буфер_повідомлення, назва);
    додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8"\": очікується \"");
    додати_ю8_до_буфера_ю8(буфер_повідомлення, обʼєкт_типу_як_ю8сі(очікується));
    додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8"\", отримано \"");
    додати_ю8_до_буфера_ю8(буфер_повідомлення, обʼєкт_типу_як_ю8сі(отримано));
    додати_ю8_до_буфера_ю8(буфер_повідомлення, ю8"\"");
    помилка_компіляції.повідомлення = створити_ю8_з_буфера(буфер_повідомлення);
    звільнити_буфер_ю8(буфер_повідомлення);
    вернути помилка_компіляції;
  }

