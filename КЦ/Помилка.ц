    додати_до_буфера_ю8(буфер_повідомлення, ю8"Не вказано тип цілі");
    помилка_компіляції.повідомлення = створити_ю8_сі_з_буфера(буфер_повідомлення);
    звільнити_буфер_ю8(буфер_повідомлення);
    вернути помилка_компіляції;
  }

  місцева дія виділити_помилку_компіляції_неможливо_визначити_ціль_з_типом_ніщо(місцезнаходження: комірка<Місцезнаходження>): комірка<ПомилкаКомпіляції> {
    ціль помилка_компіляції = виділити<ПомилкаКомпіляції>();
    помилка_компіляції.місцезнаходження = місцезнаходження;
    ціль буфер_повідомлення = створити_буфер_ю8();
    додати_до_буфера_ю8(буфер_повідомлення, ю8"Неможливо визначити ціль з типом \"ніщо\"");
    помилка_компіляції.повідомлення = створити_ю8_сі_з_буфера(буфер_повідомлення);
    звільнити_буфер_ю8(буфер_повідомлення);
    вернути помилка_компіляції;
  }

  місцева дія виділити_помилку_компіляції_шаблон_дії_може_мати_лише_внутрішню_видимість(місцезнаходження: комірка<Місцезнаходження>): комірка<ПомилкаКомпіляції> {
