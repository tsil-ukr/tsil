взяти визначення ./КЦ;

секція КЦ {
  місцева дія клонувати_шлях(компілятор: адреса<Компілятор>, шлях: Шлях) -> Шлях {
    змінна значення_копії = Шлях { шлях.розмір, виділити_памʼять<п8>(компілятор, шлях.розмір) };

    змінна п: природне = 0;
    поки п < шлях.розмір {
      значення_копії.дані[п] = шлях.дані[п];

      п += 1;
    }

    вернути значення_копії;
  }

  місцева дія знищити_шлях(компілятор: адреса<Компілятор>, шлях: Шлях) {
    звільнити_памʼять(компілятор, шлях.дані);
  }
}