взяти визначення ./КЦ;

секція КЦ {
  місцева дія знищити_назву_якщо_копія(компілятор: адреса<Компілятор>, назва: адреса<НоваНазва>) {
  }

  місцева дія змішати_назву_шлях_для_реєстру(ключ: адреса<НоваНазва>, аргумент: невідома_адреса): позитивне {
    змінна змішане: позитивне = 5381;
    змінна п: позитивне = 0;

    поки п < ключ.значення.розмір {
      змішане = (змішане * 33) + позитивне(ключ.значення.дані[п]);
      п += 1;
    }

    вернути змішане;
  }
}