взяти визначення ./КЦ;

секція КЦ {
  місцева дія створити_розібраний_текст(компілятор: адреса<Компілятор>) -> адреса<РозібранийТекст> {
    змінна розібраний_текст = виділити<РозібранийТекст>(компілятор);

    розібраний_текст.наступний = пусто;
    розібраний_текст.компілятор = компілятор;
    розібраний_текст.текст_коду = пусто;
    розібраний_текст.розбирач = пусто;
    розібраний_текст.слова = КЦ::розбирач::Слова { 0, пусто, 0 };
    розібраний_текст.сполуки = КЦ::розбирач::Сполуки { 0, пусто, 0 };

    якщо компілятор.перший_розібраний_текст == пусто {
      компілятор.перший_розібраний_текст = розібраний_текст;
    }
    якщо компілятор.останній_розібраний_текст == пусто {
      компілятор.останній_розібраний_текст = розібраний_текст;
    } інакше {
      компілятор.останній_розібраний_текст.наступний = розібраний_текст;
      компілятор.останній_розібраний_текст = розібраний_текст;
    }

    вернути розібраний_текст;
  }

  місцева дія знищити_розібраний_текст(розібраний_текст: адреса<РозібранийТекст>) {
    змінна компілятор = розібраний_текст.компілятор;

    розбирач::знищити_слова(розібраний_текст.розбирач, розібраний_текст.слова);
    розбирач::знищити_сполуки(розібраний_текст.розбирач, розібраний_текст.сполуки);
    розбирач::знищити(розібраний_текст.розбирач);

    звільнити(компілятор, розібраний_текст);
  }
}