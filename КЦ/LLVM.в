зовнішня дія tsil_llvm_make_internal_global(L: комірка<LLVM::TL>, назва: памʼять<п8>, тип: комірка<LLVM::Type>): комірка<LLVM::Value>;

секція LLVM {
  дія struct(L: комірка<TL>, назва: памʼять<п8>): комірка<Type> {
    вернути tsil_llvm_create_struct(L, назва);
  }

  дія struct_set_fields(L: комірка<TL>, с: комірка<Type>, розмір_полів: позитивне, поля: памʼять<комірка<Type>>) {
    tsil_llvm_set_struct_fields(L, с, розмір_полів, поля);
  }

  дія function(L: комірка<TL>, лінкування: позитивне, назва: памʼять<п8>, тип_повернення: комірка<Type>, розмір_параметрів: позитивне, параметри: памʼять<комірка<Type>>, варарг: позитивне): комірка<Function> {
    вернути tsil_llvm_declare_function(L, лінкування, назва, тип_повернення, розмір_параметрів, параметри, варарг);
  }

  дія block(L: комірка<TL>, функція: комірка<Function>, назва: памʼять<п8>): комірка<BasicBlock> {
    вернути tsil_llvm_create_function_block(L, функція, назва);
  }

  дія alloca(L: комірка<TL>, block: комірка<BasicBlock>, назва: памʼять<п8>, тип: комірка<Type>): комірка<Value> {
    вернути tsil_llvm_inst_alloca(L, block, назва, тип);
  }

  дія store(L: комірка<TL>, block: комірка<BasicBlock>, значення: комірка<Value>, алока: комірка<Value>) {
    tsil_llvm_inst_store(L, block, значення, алока);
  }

  дія load(L: комірка<TL>, block: комірка<BasicBlock>, тип: комірка<Type>, значення: комірка<Value>): комірка<Value> {
    вернути tsil_llvm_inst_load(L, block, тип, значення);
  }

  дія br(L: комірка<TL>, звідки: комірка<BasicBlock>, куда: комірка<BasicBlock>) {
    tsil_llvm_inst_br(L, звідки, куда);
  }

