секція LLVM {
  дія struct(L: адреса<TL>, назва: памʼять<п8>): адреса<Type> {
    вернути tsil_llvm_create_struct(L, назва);
  }

  дія struct_set_fields(L: адреса<TL>, с: адреса<Type>, розмір_полів: позитивне, поля: памʼять<адреса<Type>>) {
    tsil_llvm_set_struct_fields(L, с, розмір_полів, поля);
  }

  дія function(L: адреса<TL>, лінкування: позитивне, назва: памʼять<п8>, тип_повернення: адреса<Type>, розмір_параметрів: позитивне, параметри: памʼять<адреса<Type>>, варарг: позитивне): адреса<Function> {
    вернути tsil_llvm_declare_function(L, лінкування, назва, тип_повернення, розмір_параметрів, параметри, варарг);
  }

