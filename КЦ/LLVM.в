секція LLVM {
  синонім LINKAGE_EXTERNAL = 0;
  синонім LINKAGE_DSO_LOCAL = 1;
  синонім LINKAGE_INTERNAL = 2;

  структура TL;
  структура Type;
  структура FunctionType;
  структура Value;
  структура Function;
  структура BasicBlock;
  структура BranchInst;
}

зовнішня дія tsil_llvm_create_tl(назва: памʼять<п8>): адреса<LLVM::TL>;

зовнішня дія tsil_llvm_create_struct(L: адреса<LLVM::TL>, назва: памʼять<п8>): адреса<LLVM::Type>;
зовнішня дія tsil_llvm_set_struct_fields(L: адреса<LLVM::TL>, с: адреса<LLVM::Type>, розмір_полів: позитивне, поля: памʼять<адреса<LLVM::Type>>);

зовнішня дія tsil_llvm_declare_function(L: адреса<LLVM::TL>, лінкування: позитивне, назва: памʼять<п8>, тип_повернення: адреса<LLVM::Type>, розмір_параметрів: позитивне, параметри: памʼять<адреса<LLVM::Type>>, варарг: позитивне): адреса<LLVM::Function>;
зовнішня дія tsil_llvm_create_function_block(L: адреса<LLVM::TL>, функція: адреса<LLVM::Function>, назва: памʼять<п8>): адреса<LLVM::BasicBlock>;

