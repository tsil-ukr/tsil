  }

  дія const_float(Л: комірка<TL>, значення: д32): комірка<Value> {
    вернути tsil_llvm_create_float(Л, значення);
  }

  дія const_double(Л: комірка<TL>, значення: д64): комірка<Value> {
    вернути tsil_llvm_create_double(Л, значення);
  }

  дія type_of_function(Л: комірка<TL>, функція: комірка<Function>): комірка<FunctionType> {
    вернути tsil_llvm_get_function_type(Л, функція);
  }

  дія function_arg(Л: комірка<TL>, функція: комірка<Function>, позиція: позитивне): комірка<Value> {
    вернути tsil_llvm_get_function_arg_value(Л, функція, позиція);
  }

  дія add(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, ліво: комірка<Value>, право: комірка<Value>): комірка<Value> {
    вернути tsil_llvm_inst_add(Л, блок, ліво, право);
  }

  дія fadd(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, ліво: комірка<Value>, право: комірка<Value>): комірка<Value> {
    вернути tsil_llvm_inst_fadd(Л, блок, ліво, право);
  }

  дія sub(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, ліво: комірка<Value>, право: комірка<Value>): комірка<Value> {
    вернути tsil_llvm_inst_sub(Л, блок, ліво, право);
  }

  дія fsub(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, ліво: комірка<Value>, право: комірка<Value>): комірка<Value> {
    вернути tsil_llvm_inst_fsub(Л, блок, ліво, право);
  }

  дія mul(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, ліво: комірка<Value>, право: комірка<Value>): комірка<Value> {
    вернути tsil_llvm_inst_mul(Л, блок, ліво, право);
  }

  дія fmul(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, ліво: комірка<Value>, право: комірка<Value>): комірка<Value> {
    вернути tsil_llvm_inst_fmul(Л, блок, ліво, право);
  }

  дія udiv(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, ліво: комірка<Value>, право: комірка<Value>): комірка<Value> {
