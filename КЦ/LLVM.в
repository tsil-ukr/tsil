  дія fcmp_ogt(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, ліво: комірка<Value>, право: комірка<Value>): комірка<Value> {
    вернути tsil_llvm_inst_fcmp_ogt(Л, блок, ліво, право);
  }

  дія fcmp_ugt(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, ліво: комірка<Value>, право: комірка<Value>): комірка<Value> {
    вернути tsil_llvm_inst_fcmp_ugt(Л, блок, ліво, право);
  }
}