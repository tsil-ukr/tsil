секція LLVM {
  синонім LINKAGE_EXTERNAL = 0;
  синонім LINKAGE_DSO_LOCAL = 1;
  синонім LINKAGE_INTERNAL = 2;

  структура TL;
  структура Type;
  структура FunctionType;
  структура Value;
  структура Function;
  структура BasicBlock;
}

зовнішня дія tsil_llvm_create_tl(назва: памʼять<п8>): комірка<LLVM::TL>;
зовнішня дія dumpLL(L: комірка<LLVM::TL>): памʼять<п8>;

