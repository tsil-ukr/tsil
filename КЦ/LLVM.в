секція LLVM {
  дія struct(Л: комірка<TL>, назва: памʼять<п8>): комірка<Type> {
    вернути tsil_llvm_create_struct(Л, назва);
  }

  дія struct_set_fields(Л: комірка<TL>, с: комірка<Type>, розмір_полів: позитивне, поля: памʼять<комірка<Type>>) {
    tsil_llvm_set_struct_fields(Л, с, розмір_полів, поля);
  }

  дія function(Л: комірка<TL>, лінкування: позитивне, назва: памʼять<п8>, тип_повернення: комірка<Type>, розмір_параметрів: позитивне, параметри: памʼять<комірка<Type>>, варарг: позитивне): комірка<Function> {
    вернути tsil_llvm_declare_function(Л, лінкування, назва, тип_повернення, розмір_параметрів, параметри, варарг);
  }

  дія block(Л: комірка<TL>, функція: комірка<Function>, назва: памʼять<п8>): комірка<LLVM::BasicBlock> {
    вернути tsil_llvm_create_function_block(Л, функція, назва);
  }

  дія alloca(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, назва: памʼять<п8>, тип: комірка<Type>): комірка<Value> {
    вернути tsil_llvm_inst_alloca(Л, блок, назва, тип);
  }

  дія store(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, значення: комірка<Value>, алока: комірка<Value>) {
    tsil_llvm_inst_store(Л, блок, значення, алока);
  }

  дія load(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, тип: комірка<Type>, значення: комірка<Value>): комірка<Value> {
    вернути tsil_llvm_inst_load(Л, блок, тип, значення);
  }

  дія br(Л: комірка<TL>, звідки: комірка<BasicBlock>, куда: комірка<LLVM::BasicBlock>) {
    tsil_llvm_inst_br(Л, звідки, куда);
  }

  дія brif(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, значення: комірка<Value>, звідки: комірка<BasicBlock>, куда: комірка<LLVM::BasicBlock>) {
    tsil_llvm_inst_brif(Л, блок, значення, звідки, куда);
  }

  дія ret(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, значення: комірка<Value>) {
    tsil_llvm_inst_ret(Л, блок, значення);
  }

  дія getelementptr(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, тип: комірка<Type>, значення: комірка<Value>, кількість_позицій: позитивне, позиції: памʼять<комірка<Value>>): комірка<Value> {
    вернути tsil_llvm_inst_getelementptr(Л, блок, тип, значення, кількість_позицій, позиції);
  }

  дія call(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, тип: комірка<Type>, значення: комірка<Value>, розмір_аргументів: позитивне, аргументи: памʼять<комірка<Value>>): комірка<Value> {
    вернути tsil_llvm_inst_call_value(Л, блок, тип, значення, розмір_аргументів, аргументи);
  }

  дія null(Л: комірка<TL>): комірка<Value> {
    вернути tsil_llvm_get_null(Л);
  }

  дія void(Л: комірка<TL>): комірка<Type> {
    вернути tsil_llvm_get_void_type(Л);
  }

  дія pointer(Л: комірка<TL>): комірка<Type> {
    вернути tsil_llvm_get_pointer_type(Л);
  }

  дія і1(Л: комірка<TL>): комірка<Type> {
    вернути tsil_llvm_get_int1_type(Л);
  }

  дія і8(Л: комірка<TL>): комірка<Type> {
    вернути tsil_llvm_get_int8_type(Л);
  }

  дія і16(Л: комірка<TL>): комірка<Type> {
    вернути tsil_llvm_get_int16_type(Л);
  }

  дія і32(Л: комірка<TL>): комірка<Type> {
    вернути tsil_llvm_get_int32_type(Л);
  }

  дія і64(Л: комірка<TL>): комірка<Type> {
    вернути tsil_llvm_get_int64_type(Л);
  }

  дія f32(Л: комірка<TL>): комірка<Type> {
    вернути tsil_llvm_get_float32_type(Л);
  }

  дія f64(Л: комірка<TL>): комірка<Type> {
    вернути tsil_llvm_get_float64_type(Л);
  }

  дія const_int8(Л: комірка<TL>, значення: ц8): комірка<Value> {
    вернути tsil_llvm_create_int8(Л, значення);
  }

  дія const_int16(Л: комірка<TL>, значення: ц16): комірка<Value> {
    вернути tsil_llvm_create_int16(Л, значення);
  }

  дія const_int32(Л: комірка<TL>, значення: ц32): комірка<Value> {
    вернути tsil_llvm_create_int32(Л, значення);
  }

  дія const_int64(Л: комірка<TL>, значення: ц64): комірка<Value> {
    вернути tsil_llvm_create_int64(Л, значення);
  }

  дія const_uint1(Л: комірка<TL>, значення: п8): комірка<Value> {
    вернути tsil_llvm_create_uint1(Л, значення);
