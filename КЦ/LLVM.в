  дія srem(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, ліво: комірка<Value>, право: комірка<Value>): комірка<Value> {
    вернути tsil_llvm_inst_srem(Л, блок, ліво, право);
  }

  дія frem(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, ліво: комірка<Value>, право: комірка<Value>): комірка<Value> {
    вернути tsil_llvm_inst_frem(Л, блок, ліво, право);
  }

  дія icmp_sgt(Л: комірка<TL>, блок: комірка<LLVM::BasicBlock>, ліво: комірка<Value>, право: комірка<Value>): комірка<Value> {
    вернути tsil_llvm_inst_icmp_sgt(Л, блок, ліво, право);
