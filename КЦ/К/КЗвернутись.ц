взяти визначення К;

секція ц {
  місцева дія скомпілювати_звернутись(К: адреса<Компілятор>, інформація_середовища: ІнформаціяСередовища, місцезнаходження: адреса<Місцезнаходження>, дані: адреса<АСДДаніЗвернутись>): Результат {
    ціль назва = ю8 { позитивне(дані.ідентифікатор.розмір_значення), дані.ідентифікатор.значення };
    ціль обʼєкт: Обʼєкт;
    ціль успіх_отримання_зі_середовища = отримати_зі_середовища(інформація_середовища.середовище, назва_з_ю8(К, назва), обʼєкт::адреса);
    якщо успіх_отримання_зі_середовища == так {
//      біб::вивести_ю8(ю8сі"звернення до ");
//      біб::друк_ю8(назва);
//      біб::вивести_ю8(ю8сі"знайдено ");
//      біб::друк_ю8(обʼєкт_як_ю8сі(обʼєкт));
      вернути Результат { обʼєкт, помилка = пусто };
    }
    ціль помилка_компіляції = виділити_помилку_компіляції_субʼєкт_не_визначено(місцезнаходження, назва);
    вернути результат_помилка(помилка_компіляції);
  }
}