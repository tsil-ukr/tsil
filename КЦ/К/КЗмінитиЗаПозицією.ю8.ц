взяти визначення К;

секція ц {
  місцева дія скомпілювати_змінити_за_позицією(К: адреса<Компілятор>, інформація_середовища: ІнформаціяСередовища, місцезнаходження: адреса<Місцезнаходження>, дані: адреса<АСДДаніЗмінитиЗаПозицією>): адреса<ПомилкаКомпіляції> {
    ціль результат_компіляції_обʼєкта = скомпілювати_значення(К, інформація_середовища, дані.обʼєкт, пусто);
    якщо результат_компіляції_обʼєкта.помилка != пусто {
      вернути результат_компіляції_обʼєкта.помилка;
    }
    ціль обʼєкт = результат_компіляції_обʼєкта.обʼєкт;
    ціль результат_компіляції_позиції = скомпілювати_значення(К, інформація_середовища, дані.позиція, пусто);
    якщо результат_компіляції_позиції.помилка != пусто {
      вернути результат_компіляції_позиції.помилка;
    }
    ціль позиція = Аргумент {
      результат_компіляції_позиції.обʼєкт,
      дані.позиція.місцезнаходження
    };
    ціль результат_компіляції_значення = скомпілювати_значення(К, інформація_середовища, дані.значення, пусто);
    якщо результат_компіляції_значення.помилка != пусто {
      вернути результат_компіляції_значення.помилка;
    }
    ціль аргумент = Аргумент {
      результат_компіляції_значення.обʼєкт,
      дані.значення.місцезнаходження
    };
    ціль результат_зміни_властивості = обʼєкт_змінити_за_позицією(обʼєкт, К, інформація_середовища, місцезнаходження, позиція, аргумент);
    якщо результат_зміни_властивості.помилка != пусто {
      вернути результат_зміни_властивості.помилка;
    }
    вернути пусто;
  }
}