взяти визначення К;

секція цк {
  місцева дія скомпілювати_синонім(К: комірка<Компілятор>, блок: комірка<Блок>, інформація_виконання_шаблону: комірка<ІнформаціяВиконанняШаблону>, середовище: комірка<Середовище>, місцезнаходження: комірка<Місцезнаходження>, дані: комірка<АСДДаніСинонім>): РезультатЗначення {
    ціль назва = дані.ідентифікатор.значення;

    ціль середовище_створення_синоніма = створити_середовище(середовище);

    ціль син = виділити<Синонім>();
    син.зовнішнє_середовище = середовище_створення_синоніма;
    син.асд_значення = дані.значення;

    змінити_в_середовищі(середовище_створення_синоніма, назва, ОбʼєктКомпіляції { ВидОКСинонім, син });

    ціль обʼєкт_компіляції = ОбʼєктКомпіляції { ВидОКСинонім, син };
    ціль значення = Значення { ВидЗначенняОК, к = обʼєкт_компіляції };

    вернути РезультатЗначення { значення, пусто };
  }
}