взяти визначення К;

секція ц {
  місцева дія скомпілювати_як(К: адреса<Компілятор>, інформація_середовища: ІнформаціяСередовища, місцезнаходження: адреса<Місцезнаходження>, дані: адреса<АСДДаніЯк>, очікуваний_тип: адреса<ОбʼєктТипу>): Результат {
    ціль результат_компіляції_значення = скомпілювати_значення(К, інформація_середовища, дані.значення, очікуваний_тип);
    якщо результат_компіляції_значення.помилка != пусто {
      вернути результат_помилка(результат_компіляції_значення.помилка);
    }
    змінна ціль новий_тип: адреса<ОбʼєктТипу>;
    якщо дані.тип == пусто {
      новий_тип = очікуваний_тип;
    } інакше {
      ціль результат_компіляції_типу = скомпілювати_значення_як_тип(К, інформація_середовища, дані.тип, так);
      якщо результат_компіляції_типу.помилка != пусто {
        вернути результат_помилка(результат_компіляції_типу.помилка);
      }
      новий_тип = результат_компіляції_типу.обʼєкт_типу;
    }
    ціль обʼєкт = результат_компіляції_значення.обʼєкт;
    ціль тип_обʼєкт = Обʼєкт { ВидОбʼєктаТип, дані = новий_тип };
    вернути обʼєкт_як(обʼєкт, К, інформація_середовища, місцезнаходження, Аргумент { тип_обʼєкт, дані.тип.місцезнаходження });
  }
}