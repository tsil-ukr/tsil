взяти визначення ./КЦ;

секція КЦ {
  зовнішня дія знищити_стрибки(компілятор: адреса<Компілятор>, стрибки: Стрибки) {
    змінна п: позитивне = 0;

    поки п < стрибки.розмір {
      знищити_стрибок(компілятор, стрибки.дані[п]);

      п += 1;
    }

    звільнити_памʼять(компілятор, стрибки.дані);
  }
}