взяти визначення ./розбирач;
взяти визначення біб/Друк;

секція КЦ {
  секція розбирач {
    зовнішня дія створити(виділяч: адреса<біб::Виділяч>): адреса<Розбирач> {
      змінна розбирач = біб::виділити<Розбирач>(виділяч);
      розбирач.виділяч = виділяч;
      вернути розбирач;
    }
  }
}

секція КЦ {
  секція розбирач {
    дія визначити_розмір_символа_юнікоду(перший_байт: п8): п8 {
      якщо (перший_байт & 128) == 0 {
        вернути 1;
      }
      якщо (перший_байт & 224) == 192 {
        вернути 2;
      }
      якщо (перший_байт & 240) == 224 {
        вернути 3;
      }
      якщо (перший_байт & 248) == 240 {
        вернути 4;
      }
      вернути 0;
    }

    дія перевірити_частину_ю8(розбирач: адреса<Розбирач>, кс: ю8, значення: ю8, поточна_позиція: ПозиціяСлова, вихід_позиції_кінця: адреса<ПозиціяСлова>): логічне {
      змінна п: позитивне = 0;

    далі:
      якщо п >= кс.розмір {
        вернути ні;
      } інакше якщо поточна_позиція.позиція_в_тексті >= значення.розмір {
        вернути ні;
      } інакше {
        змінна розмір_символа_кс = визначити_розмір_символа_юнікоду(кс.дані[п]);
        змінна розмір_символа = визначити_розмір_символа_юнікоду(значення.дані[поточна_позиція.позиція_в_тексті]);

        якщо розмір_символа_кс != розмір_символа {
          вернути ні;
        }

        якщо розмір_символа == 1 {
          якщо значення.дані[поточна_позиція.позиція_в_тексті] != кс.дані[п] {
            вернути ні;
          }
          якщо п == кс.розмір - 1 {
            вихід_позиції_кінця::вміст = поточна_позиція;
            вернути так;
          }
          поточна_позиція.позиція_в_тексті += 1;
          поточна_позиція.стовпець += 1;
          п += 1;
        } інакше якщо розмір_символа == 2 {
          якщо значення.дані[поточна_позиція.позиція_в_тексті] != кс.дані[п] {
            вернути ні;
          }
          якщо п == кс.розмір - 1 {
            вернути ні;
          }
          поточна_позиція.позиція_в_тексті += 1;
          п += 1;
          якщо значення.дані[поточна_позиція.позиція_в_тексті] != кс.дані[п] {
            вернути ні;
          }
          якщо п == кс.розмір - 1 {
            вихід_позиції_кінця::вміст = поточна_позиція;
            вернути так;
          }
          поточна_позиція.позиція_в_тексті += 1;
          поточна_позиція.стовпець += 1;
          п += 1;
        } інакше якщо розмір_символа == 3 {
          якщо значення.дані[поточна_позиція.позиція_в_тексті] != кс.дані[п] {
            вернути ні;
          }
          якщо п == кс.розмір - 1 {
            вернути ні;
          }
          поточна_позиція.позиція_в_тексті += 1;
          п += 1;
          якщо значення.дані[поточна_позиція.позиція_в_тексті] != кс.дані[п] {
            вернути ні;
          }
          якщо п == кс.розмір - 1 {
            вернути ні;
          }
          поточна_позиція.позиція_в_тексті += 1;
          п += 1;
          якщо значення.дані[поточна_позиція.позиція_в_тексті] != кс.дані[п] {
            вернути ні;
          }
          якщо п == кс.розмір - 1 {
            вихід_позиції_кінця::вміст = поточна_позиція;
            вернути так;
          }
          поточна_позиція.позиція_в_тексті += 1;
          поточна_позиція.стовпець += 1;
          п += 1;
        } інакше якщо розмір_символа == 4 {
          якщо значення.дані[поточна_позиція.позиція_в_тексті] != кс.дані[п] {
            вернути ні;
          }
          якщо п == кс.розмір - 1 {
            вернути ні;
          }
          поточна_позиція.позиція_в_тексті += 1;
          п += 1;
          якщо значення.дані[поточна_позиція.позиція_в_тексті] != кс.дані[п] {
            вернути ні;
          }
          якщо п == кс.розмір - 1 {
            вернути ні;
          }
          поточна_позиція.позиція_в_тексті += 1;
          п += 1;
          якщо значення.дані[поточна_позиція.позиція_в_тексті] != кс.дані[п] {
            вернути ні;
          }
          якщо п == кс.розмір - 1 {
            вернути ні;
          }
          поточна_позиція.позиція_в_тексті += 1;
          п += 1;
          якщо значення.дані[поточна_позиція.позиція_в_тексті] != кс.дані[п] {
            вернути ні;
          }
          якщо п == кс.розмір - 1 {
            вихід_позиції_кінця::вміст = поточна_позиція;
            вернути так;
          }
          поточна_позиція.позиція_в_тексті += 1;
          поточна_позиція.стовпець += 1;
          п += 1;
        } інакше {
          вернути ні;
        }

        стрибнути далі;
      }

      вернути ні;
    }

    дія перевірити_чи_текст_ю8(розбирач: адреса<Розбирач>, значення: ю8, поточна_позиція: ПозиціяСлова, вихід_позиції_кінця: адреса<ПозиціяСлова>): логічне {
      якщо значення.дані[поточна_позиція.позиція_в_тексті] != 34 {
        вернути ні;
      }

      змінна закінчено = ні;
      змінна екран = ні;

      поточна_позиція.позиція_в_тексті += 1;
      поточна_позиція.стовпець += 1;

    далі:
      якщо поточна_позиція.позиція_в_тексті >= значення.розмір {
        вернути ні;
      } інакше {
        якщо значення.дані[поточна_позиція.позиція_в_тексті] == 92 {
          якщо екран {
            екран = ні;
          } інакше {
            екран = так;
          }
        } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 34 {
          якщо екран {
            екран = ні;
          } інакше {
            закінчено = так;
            стрибнути вихід;
          }
        } інакше {
          екран = ні;
        }

        змінна розмір_символа = визначити_розмір_символа_юнікоду(значення.дані[поточна_позиція.позиція_в_тексті]);

        якщо розмір_символа == 1 {
          поточна_позиція.позиція_в_тексті += 1;
          поточна_позиція.стовпець += 1;
        } інакше якщо розмір_символа == 2 {
          поточна_позиція.позиція_в_тексті += 2;
          поточна_позиція.стовпець += 1;
        } інакше якщо розмір_символа == 3 {
          поточна_позиція.позиція_в_тексті += 3;
          поточна_позиція.стовпець += 1;
        } інакше якщо розмір_символа == 4 {
          поточна_позиція.позиція_в_тексті += 4;
          поточна_позиція.стовпець += 1;
        } інакше {
          вернути ні;
        }

        стрибнути далі;
      }

    вихід:
      якщо закінчено {
        вихід_позиції_кінця::вміст = поточна_позиція;
        вернути так;
      } інакше {
        вернути ні;
      }
    }

    дія перевірити_чи_число_ю8(розбирач: адреса<Розбирач>, значення: ю8, поточна_позиція: ПозиціяСлова, вихід_позиції_кінця: адреса<ПозиціяСлова>): логічне {
      якщо значення.дані[поточна_позиція.позиція_в_тексті] == 48 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 49 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 50 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 51 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 52 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 53 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 54 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 55 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 56 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 57 {
      } інакше {
        вернути ні;
      }

      поточна_позиція.позиція_в_тексті += 1;
      поточна_позиція.стовпець += 1;

      змінна з_крапкою = ні;
      змінна цифра_після_крапки = ні;

    продовження:
      якщо значення.дані[поточна_позиція.позиція_в_тексті] == 46 {
        якщо з_крапкою {
          вернути ні;
        } інакше {
          з_крапкою = так;
        }
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 48 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 49 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 50 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 51 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 52 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 53 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 54 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 55 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 56 {
      } інакше якщо значення.дані[поточна_позиція.позиція_в_тексті] == 57 {
      } інакше {
        якщо з_крапкою {
          якщо цифра_після_крапки == ні {
            вернути ні;
          }
        }
        поточна_позиція.позиція_в_тексті -= 1;
        поточна_позиція.стовпець -= 1;
        вихід_позиції_кінця::вміст = поточна_позиція;
        вернути так;
      }

      якщо з_крапкою {
        цифра_після_крапки = так;
      }

    далі:
      поточна_позиція.позиція_в_тексті += 1;
      поточна_позиція.стовпець += 1;

      стрибнути продовження;
    }

    дія перевірити_чи_початок_ідентифікатора_ю8(розбирач: адреса<Розбирач>, значення: ю8, поточна_позиція: ПозиціяСлова, вихід_позиції_кінця: адреса<ПозиціяСлова>): логічне {
      якщо перевірити_частину_ю8(розбирач, ю8"ʼ", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"_", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"А", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Б", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"В", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Г", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Д", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Е", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Є", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Ж", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"З", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"И", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"І", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Ї", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Й", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"К", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Л", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"М", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Н", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"О", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"П", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Р", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"С", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Т", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"У", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Ф", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Х", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Ц", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Ч", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Ш", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Щ", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Ь", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Ю", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Я", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"Ґ", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"а", значення, поточна_позиція, вихід_позиції_кінця) {
      } інакше якщо перевірити_частину_ю8(розбирач, ю8"б", значення, поточна_позиція, вихід_позиції_кінця) {
