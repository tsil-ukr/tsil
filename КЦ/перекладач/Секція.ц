взяти визначення КЦ;

секція КЦ {
  місцева дія скомпілювати_секцію(компілятор: адреса<Компілятор>, контекст: Контекст, контекст_виконання_шаблону: адреса<КонтекстКомпіляціїВиконанняШаблону>, дані_секції: розбирач::ДаніСполукиСекція, місцезнаходження: розбирач::Місцезнаходження) -> РезультатОбʼєктаСекції {
    змінна назва_т8 = т8 { 0, пусто };
    
    якщо не розбирач::отримати_значення_слова(
      ю8 { контекст.розібраний_текст.текст_коду.значення.розмір, контекст.розібраний_текст.текст_коду.значення.дані },
      контекст.розібраний_текст.слова.дані[дані_секції.ідентифікатор.значення.позиція_слова],
      назва_т8::адреса
    ) {
      вернути РезультатОбʼєктаСекції {
        ..., 
        помилка = створити_помилку(
          компілятор,
          "Не вдалось отримати назву секції.",
          створити_місцезнаходження_помилки_з_місцезнаходження_розбирача(компілятор, контекст.розібраний_текст, місцезнаходження)
        )
      };
    }

    змінна назва = назва_без_копіювання(компілятор, назва_т8);

    змінна обʼєкт_секції = виділити_обʼєкт_секції(компілятор);

    обʼєкт_секції.назва = назва;
    обʼєкт_секції.середовище = створити_середовище_секції(компілятор, контекст.середовище, обʼєкт_секції);

    змінна результат_компіляції_тіла = скомпілювати_тіло(
      компілятор,
      Контекст {
        розібраний_текст = контекст.розібраний_текст,
        середовище = обʼєкт_секції.середовище,
        кк_дії = контекст.кк_дії
      },
      дані_секції.тіло,
      місцезнаходження
    );

    якщо результат_компіляції_тіла.помилка != пусто {
      вернути РезультатОбʼєктаСекції { ..., помилка = результат_компіляції_тіла.помилка };
    }

    вернути РезультатОбʼєктаСекції {
      обʼєкт_секції,
      помилка = пусто
    };
  }

  місцева дія скомпілювати_визначити_секцію(компілятор: адреса<Компілятор>, контекст: Контекст, дані_секції: розбирач::ДаніСполукиСекція, місцезнаходження: розбирач::Місцезнаходження) -> РезультатОбʼєктаСекції {
    змінна назва_т8 = т8 { 0, пусто };
    
    якщо не розбирач::отримати_значення_слова(
      ю8 { контекст.розібраний_текст.текст_коду.значення.розмір, контекст.розібраний_текст.текст_коду.значення.дані },
      контекст.розібраний_текст.слова.дані[дані_секції.ідентифікатор.значення.позиція_слова],
      назва_т8::адреса
    ) {
      вернути РезультатОбʼєктаСекції {
        ..., 
        помилка = створити_помилку(
          компілятор,
          "Не вдалось отримати назву секції.",
          створити_місцезнаходження_помилки_з_місцезнаходження_розбирача(компілятор, контекст.розібраний_текст, місцезнаходження)
        )
      };
    }

    змінна назва = назва_без_копіювання(компілятор, назва_т8);

    змінна обʼєкт_з_середовища: Обʼєкт;
    якщо знайти_в_середовищі(контекст.середовище, назва, обʼєкт_з_середовища::адреса) {
      якщо обʼєкт_з_середовища.вид == ВидОбʼєктаСекція {
        змінна обʼєкт_секції = обʼєкт_з_середовища.дані як адреса<ОбʼєктСекції>;

        змінна результат_компіляції_тіла = скомпілювати_тіло(
          компілятор,
          Контекст {
            розібраний_текст = контекст.розібраний_текст,
            середовище = обʼєкт_секції.середовище,
            кк_дії = контекст.кк_дії
          },
          дані_секції.тіло,
          місцезнаходження
        );

        якщо результат_компіляції_тіла.помилка != пусто {
          вернути РезультатОбʼєктаСекції { ..., помилка = результат_компіляції_тіла.помилка };
        }

        вернути РезультатОбʼєктаСекції { ..., помилка = пусто };
      }

      вернути РезультатОбʼєктаСекції {
        ...,
        помилка = створити_помилку(
          компілятор,
          "Субʼєкт вже визначено.",
          створити_місцезнаходження_помилки_з_позиції_слова(компілятор, контекст.розібраний_текст, дані_секції.ідентифікатор.значення.позиція_слова)
        )
      };
    }

    змінна результат = скомпілювати_секцію(
      компілятор,
      контекст,
      пусто,
      дані_секції,
      місцезнаходження
    );

    якщо результат.помилка != пусто {
      вернути результат;
    }

    змінити_в_середовищі(контекст.середовище, назва, Обʼєкт { ВидОбʼєктаСекція, результат.обʼєкт_секції });

    вернути результат;
  }
}