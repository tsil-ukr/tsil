взяти визначення КЦ;

секція КЦ {
  зовнішня дія скомпілювати_синонім(компілятор: адреса<Компілятор>, контекст: Контекст, дані_синоніма: розбирач::ДаніСполукиСинонім, місцезнаходження: розбирач::Місцезнаходження): РезультатОбʼєкта {
    вернути скомпілювати_значення(
      компілятор,
      контекст,
      дані_синоніма.значення,
      пусто
    );
  }

  зовнішня дія скомпілювати_визначити_синонім(компілятор: адреса<Компілятор>, контекст: Контекст, дані_синоніма: розбирач::ДаніСполукиСинонім, місцезнаходження: розбирач::Місцезнаходження): РезультатОбʼєкта {
    змінна назва_т8 = розбирач::отримати_значення_слова(
      т8 { контекст.розібраний_текст.текст_коду.значення.розмір, контекст.розібраний_текст.текст_коду.значення.дані },
      контекст.розібраний_текст.слова.дані[дані_синоніма.ідентифікатор.позиція_слова]
    );

    змінна обʼєкт_з_середовища: Обʼєкт;
    якщо знайти_в_середовищі(контекст.середовище, назва_зовнішня(назва_т8), обʼєкт_з_середовища::адреса) {
      вернути РезультатОбʼєкта {
        ...,
        помилка = створити_помилку(
          компілятор,
          "Субʼєкт вже визначено.",
          створити_місцезнаходження_помилки_з_позиції_слова(компілятор, контекст.розібраний_текст, дані_синоніма.ідентифікатор.позиція_слова)
        )
      };
    }

    змінна результат = скомпілювати_синонім(
      компілятор,
      Контекст {
        розібраний_текст = контекст.розібраний_текст,
        середовище = контекст.середовище,
        обʼєкт_секції = контекст.обʼєкт_секції,
        кк_дії = пусто,
        кк_виконання_шаблону = пусто
      },
      дані_синоніма,
      місцезнаходження
    );

    якщо результат.помилка != пусто {
      вернути результат;
    }

    змінити_в_середовищі(контекст.середовище, назва_зовнішня(назва_т8), результат.обʼєкт);

    вернути результат;
  }
}