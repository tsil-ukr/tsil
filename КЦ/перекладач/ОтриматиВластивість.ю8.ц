взяти визначення КЦ;

секція КЦ {
  зовнішня дія скомпілювати_отримати_властивість(компілятор: адреса<Компілятор>, контекст: Контекст, дані_отримати_властивість: розбирач::ДаніСполукиОтриматиВластивість, місцезнаходження: розбирач::Місцезнаходження, очікуваний_тип: адреса<ОбʼєктТипу>): РезультатОбʼєкта {
    змінна результат_ліво = скомпілювати_значення(компілятор, контекст, дані_отримати_властивість.ліво, пусто);
    якщо результат_ліво.помилка != пусто {
      вернути РезультатОбʼєкта { ..., помилка = результат_ліво.помилка };
    }

    змінна назва_ю8 = розбирач::отримати_значення_ідентифікатора(
      ю8 { контекст.розібраний_текст.текст_коду.значення.розмір, контекст.розібраний_текст.текст_коду.значення.дані },
      контекст.розібраний_текст.слова,
      дані_отримати_властивість.ідентифікатор
    );
    змінна назва_т8 = отримати_т8_з_ю8(компілятор, назва_ю8);

    змінна результат = обʼєкт_отримати_властивість(
      компілятор,
      контекст,
      місцезнаходження,
      результат_ліво.обʼєкт,
      назва_зовнішня(назва_т8),
      очікуваний_тип
    );

    звільнити_памʼять(компілятор, назва_т8.дані);

    вернути результат;
  }
}