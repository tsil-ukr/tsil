взяти визначення КЦ;

секція КЦ {
  зовнішня дія скомпілювати_змінити_елемент(компілятор: адреса<Компілятор>, контекст: Контекст, дані_змінити_елемент: розбирач::ДаніСполукиЗмінитиЕлемент, місцезнаходження: розбирач::Місцезнаходження): РезультатОбʼєкта {
    змінна результат_ліво = скомпілювати_значення(компілятор, контекст, пусто, дані_змінити_елемент.ліво, пусто);
    якщо результат_ліво.помилка != пусто {
      вернути РезультатОбʼєкта { ..., помилка = результат_ліво.помилка };
    }

    змінна результат_ключа = скомпілювати_значення(компілятор, контекст, пусто, дані_змінити_елемент.ключ, пусто);
    якщо результат_ключа.помилка != пусто {
      вернути РезультатОбʼєкта { ..., помилка = результат_ключа.помилка };
    }

    змінна результат_значення = скомпілювати_значення(компілятор, контекст, пусто, дані_змінити_елемент.значення, пусто);
    якщо результат_значення.помилка != пусто {
      вернути РезультатОбʼєкта { ..., помилка = результат_значення.помилка };
    }

    змінна результат = обʼєкт_змінити_за_позицією(
      компілятор,
      контекст,
      місцезнаходження,
      результат_ліво.обʼєкт,
      Аргумент { результат_ключа.обʼєкт, дані_змінити_елемент.ключ.місцезнаходження },
      Аргумент { результат_значення.обʼєкт, дані_змінити_елемент.значення.місцезнаходження }
    );

    вернути результат;
  }
}