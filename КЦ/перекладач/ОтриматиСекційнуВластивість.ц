взяти визначення КЦ;

секція КЦ {
  місцева дія скомпілювати_отримати_секційну_властивість(компілятор: адреса<Компілятор>, контекст: Контекст, дані_отримати_секційну_властивість: розбирач::ДаніСполукиОтриматиСекційнуВластивість, місцезнаходження: розбирач::Місцезнаходження, очікуваний_тип: адреса<ОбʼєктТипу>): РезультатОбʼєкта {
    змінна результат_ліво = скомпілювати_значення(компілятор, контекст, пусто, дані_отримати_секційну_властивість.ліво, пусто);
    якщо результат_ліво.помилка != пусто {
      вернути РезультатОбʼєкта { ..., помилка = результат_ліво.помилка };
    }

    змінна назва_т8 = розбирач::отримати_значення_ідентифікатора(
      т8 { контекст.розібраний_текст.текст_коду.значення.розмір, контекст.розібраний_текст.текст_коду.значення.дані },
      контекст.розібраний_текст.слова,
      дані_отримати_секційну_властивість.ідентифікатор
    );

    змінна результат = обʼєкт_отримати_властивість_секції(
      компілятор,
      контекст,
      місцезнаходження,
      результат_ліво.обʼєкт,
      назва_зовнішня(назва_т8),
      очікуваний_тип
    );

    вернути результат;
  }
}