взяти визначення КЦ;

секція КЦ {
  місцева дія скомпілювати_взяти(компілятор: адреса<Компілятор>, контекст: Контекст, дані_взяти: розбирач::ДаніСполукиВзяти, місцезнаходження: розбирач::Місцезнаходження): РезультатОбʼєкта {
    змінна накопичувач_шляху_1 = зробити_накопичувач_шляху(компілятор);
    змінна накопичувач_шляху_2 = зробити_накопичувач_шляху(компілятор);
    змінна накопичувач_шляху_3 = зробити_накопичувач_шляху(компілятор);
    змінна накопичувач_шляху_4 = зробити_накопичувач_шляху(компілятор);
    змінна накопичувач_шляху_5 = зробити_накопичувач_шляху(компілятор);
    змінна накопичувач_шляху_6 = зробити_накопичувач_шляху(компілятор);

    змінна звідки_т8 = т8 { 0, пусто };
    
    якщо не розбирач::отримати_значення_слова(
      ю8 { контекст.розібраний_текст.текст_коду.значення.розмір, контекст.розібраний_текст.текст_коду.значення.дані },
      контекст.розібраний_текст.слова.дані[дані_взяти.звідки.позиція_слова],
      звідки_т8::адреса
    ) {
      вернути РезультатОбʼєкта {
        помилка = створити_помилку(
          компілятор,
          "Не вдалось отримати значення слова для звідки.",
          створити_місцезнаходження_помилки_з_місцезнаходження_розбирача(компілятор, контекст.розібраний_текст, місцезнаходження)
        ),
        ...
      };
    }

    змінна директорія = Шлях { 0, пусто };
    змінна директорію_треба_звільнити = ні;

    якщо біб::перевірити_чи_т8_рівні(звідки_т8, константа_Т8_визначення()) або біб::перевірити_чи_т8_рівні(звідки_т8, константа_Т8_заявлення()) {
      якщо дані_взяти.відносно == позитивне(так) {
        якщо не отримати_директорію_шляху_до_файлу(компілятор, контекст.розібраний_текст.текст_коду.шлях, директорія::адреса) {
          вернути РезультатОбʼєкта {
            помилка = створити_помилку(
              компілятор,
              "Не вдалось отримати директорію шляху до файлу.",
              створити_місцезнаходження_помилки_з_місцезнаходження_розбирача(компілятор, контекст.розібраний_текст, місцезнаходження)
            ),
            ...
          };
        }
        директорію_треба_звільнити = так;
      } інакше {
        директорія = компілятор.кореневий_шлях;
      }
    } інакше якщо біб::перевірити_чи_т8_рівні(звідки_т8, константа_Т8_біб()) {
      якщо дані_взяти.відносно == позитивне(так) {
        вернути РезультатОбʼєкта {
          помилка = створити_помилку(
            компілятор,
            "Неможливо взяти біб відносно.",
            створити_місцезнаходження_помилки_з_місцезнаходження_розбирача(компілятор, контекст.розібраний_текст, місцезнаходження)
          ),
          ...
        };
      }

      якщо компілятор.шлях_до_біб.розмір == 0 {
        якщо дані_взяти.відносно == позитивне(так) {
          вернути РезультатОбʼєкта {
            помилка = створити_помилку(
              компілятор,
              "Шлях до біб не вказано.",
              створити_місцезнаходження_помилки_з_місцезнаходження_розбирача(компілятор, контекст.розібраний_текст, місцезнаходження)
            ),
            ...
          };
        }
      }
      
      директорія = компілятор.шлях_до_біб;
    }

    звільнити_памʼять(компілятор, звідки_т8.дані);

    покласти_в_накопичувач_шляху(компілятор, накопичувач_шляху_1::адреса, директорія);
    покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_1::адреса, константа_Т8__розділювач_шляху());
    покласти_в_накопичувач_шляху(компілятор, накопичувач_шляху_2::адреса, директорія);
    покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_2::адреса, константа_Т8__розділювач_шляху());
    покласти_в_накопичувач_шляху(компілятор, накопичувач_шляху_3::адреса, директорія);
    покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_3::адреса, константа_Т8__розділювач_шляху());
    покласти_в_накопичувач_шляху(компілятор, накопичувач_шляху_4::адреса, директорія);
    покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_4::адреса, константа_Т8__розділювач_шляху());
    покласти_в_накопичувач_шляху(компілятор, накопичувач_шляху_5::адреса, директорія);
    покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_5::адреса, константа_Т8__розділювач_шляху__визначення());
    покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_5::адреса, константа_Т8__розділювач_шляху());
    покласти_в_накопичувач_шляху(компілятор, накопичувач_шляху_6::адреса, директорія);
    покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_6::адреса, константа_Т8__розділювач_шляху__заявлення());
    покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_6::адреса, константа_Т8__розділювач_шляху());
    
    якщо директорію_треба_звільнити {
      звільнити_памʼять(компілятор, директорія.дані);
    }

    змінна п: позитивне = 0;

    поки п < дані_взяти.шлях.розмір {
      змінна частина_т8 = т8 { 0, пусто };
      
      якщо не розбирач::отримати_значення_слова(
        ю8 { контекст.розібраний_текст.текст_коду.значення.розмір, контекст.розібраний_текст.текст_коду.значення.дані },
        контекст.розібраний_текст.слова.дані[дані_взяти.шлях.дані[п].позиція_слова],
        частина_т8::адреса
      ) {
        вернути РезультатОбʼєкта {
          помилка = створити_помилку(
            компілятор,
            "Не вдалось отримати значення слова для частини шляху.",
            створити_місцезнаходження_помилки_з_місцезнаходження_розбирача(компілятор, контекст.розібраний_текст, місцезнаходження)
          ),
          ...
        };
      }

      покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_1::адреса, частина_т8);
      покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_2::адреса, частина_т8);
      покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_3::адреса, частина_т8);
      покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_4::адреса, частина_т8);
      покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_5::адреса, частина_т8);
      покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_6::адреса, частина_т8);

      якщо п != дані_взяти.шлях.розмір - 1 {
        покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_1::адреса, константа_Т8__розділювач_шляху());
        покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_2::адреса, константа_Т8__розділювач_шляху());
        покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_3::адреса, константа_Т8__розділювач_шляху());
        покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_4::адреса, константа_Т8__розділювач_шляху());
        покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_5::адреса, константа_Т8__розділювач_шляху());
        покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_6::адреса, константа_Т8__розділювач_шляху());
      }

      якщо п == дані_взяти.шлях.розмір - 1 {
        покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_2::адреса, константа_Т8__розділювач_шляху());
        покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_2::адреса, частина_т8);
        покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_4::адреса, константа_Т8__розділювач_шляху());
        покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_4::адреса, частина_т8);
      }

      звільнити_памʼять(компілятор, частина_т8.дані);

      п += 1;
    }

    покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_1::адреса, константа_Т8__закінчення_шляху__вц());
    покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_2::адреса, константа_Т8__закінчення_шляху__вц());
    покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_3::адреса, константа_Т8__закінчення_шляху__зц());
    покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_4::адреса, константа_Т8__закінчення_шляху__зц());
    покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_5::адреса, константа_Т8__закінчення_шляху__вц());
    покласти_т8_в_накопичувач_шляху(компілятор, накопичувач_шляху_6::адреса, константа_Т8__закінчення_шляху__зц());

    змінна шлях_1 = Шлях { накопичувач_шляху_1.значення.розмір, накопичувач_шляху_1.значення.дані };
    змінна шлях_2 = Шлях { накопичувач_шляху_2.значення.розмір, накопичувач_шляху_2.значення.дані };
    змінна шлях_3 = Шлях { накопичувач_шляху_3.значення.розмір, накопичувач_шляху_3.значення.дані };
    змінна шлях_4 = Шлях { накопичувач_шляху_4.значення.розмір, накопичувач_шляху_4.значення.дані };
    змінна шлях_5 = Шлях { накопичувач_шляху_5.значення.розмір, накопичувач_шляху_5.значення.дані };
    змінна шлях_6 = Шлях { накопичувач_шляху_6.значення.розмір, накопичувач_шляху_6.значення.дані };

    змінна помилка: адреса<Помилка> = пусто;

    якщо перевірити_чи_шлях_існує(компілятор, шлях_1) {
      змінна абсолютний_шлях_1 = Шлях { 0, пусто };

      якщо отримати_абсолютний_шлях(компілятор, шлях_1, абсолютний_шлях_1::адреса) == ні {
        вернути РезультатОбʼєкта {
          помилка = створити_помилку(
            компілятор,
            "Не вдалось отримати абсолютний шлях.",
            створити_місцезнаходження_помилки_з_місцезнаходження_розбирача(компілятор, контекст.розібраний_текст, місцезнаходження)
          ),
          ...
        };
      }

      звільнити_памʼять(компілятор, шлях_1.дані);
      звільнити_памʼять(компілятор, шлях_2.дані);
      звільнити_памʼять(компілятор, шлях_3.дані);
      звільнити_памʼять(компілятор, шлях_4.дані);
      звільнити_памʼять(компілятор, шлях_5.дані);
      звільнити_памʼять(компілятор, шлях_6.дані);

      якщо взяти_файл_ю8(компілятор, контекст.середовище, абсолютний_шлях_1, помилка::адреса) == ні {
        звільнити_памʼять(компілятор, абсолютний_шлях_1.дані);
        вернути РезультатОбʼєкта { помилка = помилка, ... };
      }
      звільнити_памʼять(компілятор, абсолютний_шлях_1.дані);
    } інакше якщо перевірити_чи_шлях_існує(компілятор, шлях_2) {
      змінна абсолютний_шлях_2 = Шлях { 0, пусто };

      якщо отримати_абсолютний_шлях(компілятор, шлях_2, абсолютний_шлях_2::адреса) == ні {
        вернути РезультатОбʼєкта {
          помилка = створити_помилку(
            компілятор,
            "Не вдалось отримати абсолютний шлях.",
            створити_місцезнаходження_помилки_з_місцезнаходження_розбирача(компілятор, контекст.розібраний_текст, місцезнаходження)
          ),
          ...
        };
      }

      звільнити_памʼять(компілятор, шлях_1.дані);
      звільнити_памʼять(компілятор, шлях_2.дані);
      звільнити_памʼять(компілятор, шлях_3.дані);
      звільнити_памʼять(компілятор, шлях_4.дані);
      звільнити_памʼять(компілятор, шлях_5.дані);
      звільнити_памʼять(компілятор, шлях_6.дані);

      якщо взяти_файл_ю8(компілятор, контекст.середовище, абсолютний_шлях_2, помилка::адреса) == ні {
        звільнити_памʼять(компілятор, абсолютний_шлях_2.дані);
        вернути РезультатОбʼєкта { помилка = помилка, ... };
      }
      звільнити_памʼять(компілятор, абсолютний_шлях_2.дані);
    } інакше якщо перевірити_чи_шлях_існує(компілятор, шлях_3) {
      змінна абсолютний_шлях_3 = Шлях { 0, пусто };

      якщо отримати_абсолютний_шлях(компілятор, шлях_3, абсолютний_шлях_3::адреса) == ні {
        вернути РезультатОбʼєкта {
          помилка = створити_помилку(
            компілятор,
            "Не вдалось отримати абсолютний шлях.",
            створити_місцезнаходження_помилки_з_місцезнаходження_розбирача(компілятор, контекст.розібраний_текст, місцезнаходження)
          ),
          ...
        };
      }

      звільнити_памʼять(компілятор, шлях_1.дані);
      звільнити_памʼять(компілятор, шлях_2.дані);
      звільнити_памʼять(компілятор, шлях_3.дані);
      звільнити_памʼять(компілятор, шлях_4.дані);
      звільнити_памʼять(компілятор, шлях_5.дані);
      звільнити_памʼять(компілятор, шлях_6.дані);

      якщо взяти_файл_ю8(компілятор, контекст.середовище, абсолютний_шлях_3, помилка::адреса) == ні {
        звільнити_памʼять(компілятор, абсолютний_шлях_3.дані);
        вернути РезультатОбʼєкта { помилка = помилка, ... };
      }
      звільнити_памʼять(компілятор, абсолютний_шлях_3.дані);
    } інакше якщо перевірити_чи_шлях_існує(компілятор, шлях_4) {
      змінна абсолютний_шлях_4 = Шлях { 0, пусто };

      якщо отримати_абсолютний_шлях(компілятор, шлях_4, абсолютний_шлях_4::адреса) == ні {
        вернути РезультатОбʼєкта {
          помилка = створити_помилку(
            компілятор,
            "Не вдалось отримати абсолютний шлях.",
            створити_місцезнаходження_помилки_з_місцезнаходження_розбирача(компілятор, контекст.розібраний_текст, місцезнаходження)
          ),
          ...
        };
      }

      звільнити_памʼять(компілятор, шлях_1.дані);
      звільнити_памʼять(компілятор, шлях_2.дані);
      звільнити_памʼять(компілятор, шлях_3.дані);
      звільнити_памʼять(компілятор, шлях_4.дані);
      звільнити_памʼять(компілятор, шлях_5.дані);
      звільнити_памʼять(компілятор, шлях_6.дані);

      якщо взяти_файл_ю8(компілятор, контекст.середовище, абсолютний_шлях_4, помилка::адреса) == ні {
        звільнити_памʼять(компілятор, абсолютний_шлях_4.дані);
        вернути РезультатОбʼєкта { помилка = помилка, ... };
      }
      звільнити_памʼять(компілятор, абсолютний_шлях_4.дані);
    } інакше якщо перевірити_чи_шлях_існує(компілятор, шлях_5) {
      змінна абсолютний_шлях_5 = Шлях { 0, пусто };

      якщо отримати_абсолютний_шлях(компілятор, шлях_5, абсолютний_шлях_5::адреса) == ні {
        вернути РезультатОбʼєкта {
          помилка = створити_помилку(
            компілятор,
            "Не вдалось отримати абсолютний шлях.",
            створити_місцезнаходження_помилки_з_місцезнаходження_розбирача(компілятор, контекст.розібраний_текст, місцезнаходження)
          ),
          ...
        };
      }

      звільнити_памʼять(компілятор, шлях_1.дані);
      звільнити_памʼять(компілятор, шлях_2.дані);
      звільнити_памʼять(компілятор, шлях_3.дані);
      звільнити_памʼять(компілятор, шлях_4.дані);
      звільнити_памʼять(компілятор, шлях_5.дані);

      якщо взяти_файл_ю8(компілятор, контекст.середовище, абсолютний_шлях_5, помилка::адреса) == ні {
        звільнити_памʼять(компілятор, абсолютний_шлях_5.дані);
        вернути РезультатОбʼєкта { помилка = помилка, ... };
      }
      звільнити_памʼять(компілятор, абсолютний_шлях_5.дані);
    } інакше якщо перевірити_чи_шлях_існує(компілятор, шлях_6) {
      змінна абсолютний_шлях_6 = Шлях { 0, пусто };

      якщо отримати_абсолютний_шлях(компілятор, шлях_6, абсолютний_шлях_6::адреса) == ні {
        вернути РезультатОбʼєкта {
          помилка = створити_помилку(
            компілятор,
            "Не вдалось отримати абсолютний шлях.",
            створити_місцезнаходження_помилки_з_місцезнаходження_розбирача(компілятор, контекст.розібраний_текст, місцезнаходження)
          ),
          ...
        };
      }

      звільнити_памʼять(компілятор, шлях_1.дані);
      звільнити_памʼять(компілятор, шлях_2.дані);
      звільнити_памʼять(компілятор, шлях_3.дані);
      звільнити_памʼять(компілятор, шлях_4.дані);
      звільнити_памʼять(компілятор, шлях_5.дані);
      звільнити_памʼять(компілятор, шлях_6.дані);

      якщо взяти_файл_ю8(компілятор, контекст.середовище, абсолютний_шлях_6, помилка::адреса) == ні {
        звільнити_памʼять(компілятор, абсолютний_шлях_6.дані);
        вернути РезультатОбʼєкта { помилка = помилка, ... };
      }
      звільнити_памʼять(компілятор, абсолютний_шлях_6.дані);
    } інакше {
      звільнити_памʼять(компілятор, шлях_1.дані);
      звільнити_памʼять(компілятор, шлях_2.дані);
      звільнити_памʼять(компілятор, шлях_3.дані);
      звільнити_памʼять(компілятор, шлях_4.дані);
      звільнити_памʼять(компілятор, шлях_5.дані);
      звільнити_памʼять(компілятор, шлях_6.дані);

      вернути РезультатОбʼєкта {
        помилка = створити_помилку(
          компілятор,
          "Шлях взяття не існує.",
          створити_місцезнаходження_помилки_з_місцезнаходження_розбирача(компілятор, контекст.розібраний_текст, місцезнаходження)
        ),
        ...
      };
    }

    вернути РезультатОбʼєкта { помилка = пусто, ... };
  }
}