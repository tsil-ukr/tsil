взяти визначення КЦ;

секція КЦ {
  зовнішня дія скомпілювати_секцію(компілятор: адреса<Компілятор>, контекст: Контекст, дані_секції: розбирач::ДаніСполукиСекція, місцезнаходження: розбирач::Місцезнаходження): РезультатОбʼєктаСекції {
    змінна назва_т8 = розбирач::отримати_значення_слова(
      т8 { контекст.розібраний_текст.текст_коду.значення.розмір, контекст.розібраний_текст.текст_коду.значення.дані },
      контекст.розібраний_текст.слова.дані[дані_секції.ідентифікатор.значення.позиція_слова]
    );

    змінна обʼєкт_секції = виділити_обʼєкт_секції(компілятор);

    обʼєкт_секції.назва = назва_зовнішня(назва_т8);
    обʼєкт_секції.середовище = створити_середовище(компілятор, контекст.середовище);

    змінна результат_компіляції_тіла = скомпілювати_тіло(
      компілятор,
      Контекст {
        розібраний_текст = контекст.розібраний_текст,
        середовище = обʼєкт_секції.середовище,
        обʼєкт_секції = обʼєкт_секції,
        кк_дії = пусто,
        кк_виконання_шаблону = пусто
      },
      дані_секції.тіло,
      місцезнаходження
    );

    якщо результат_компіляції_тіла.помилка != пусто {
      вернути РезультатОбʼєктаСекції { ..., помилка = результат_компіляції_тіла.помилка };
    }

    вернути РезультатОбʼєктаСекції {
      обʼєкт_секції,
      помилка = пусто
    };
  }

  зовнішня дія скомпілювати_визначити_секцію(компілятор: адреса<Компілятор>, контекст: Контекст, дані_секції: розбирач::ДаніСполукиСекція, місцезнаходження: розбирач::Місцезнаходження): РезультатОбʼєктаСекції {
    змінна назва_т8 = розбирач::отримати_значення_слова(
      т8 { контекст.розібраний_текст.текст_коду.значення.розмір, контекст.розібраний_текст.текст_коду.значення.дані },
      контекст.розібраний_текст.слова.дані[дані_секції.ідентифікатор.значення.позиція_слова]
    );

    змінна обʼєкт_з_середовища: Обʼєкт;
    якщо знайти_в_середовищі(контекст.середовище, назва_зовнішня(назва_т8), обʼєкт_з_середовища::адреса) {
      якщо обʼєкт_з_середовища.вид == ВидОбʼєктаСекція {
        змінна обʼєкт_секції = обʼєкт_з_середовища.дані як адреса<ОбʼєктСекції>;

        // ...
      }

      вернути РезультатОбʼєктаСекції {
        ...,
        помилка = створити_помилку(
          компілятор,
          "Субʼєкт вже визначено.",
          створити_місцезнаходження_помилки_з_позиції_слова(компілятор, контекст.розібраний_текст, дані_секції.ідентифікатор.значення.позиція_слова)
        )
      };
    }

    змінна результат = скомпілювати_секцію(
      компілятор,
      Контекст {
        розібраний_текст = контекст.розібраний_текст,
        середовище = контекст.середовище,
        обʼєкт_секції = контекст.обʼєкт_секції,
        кк_дії = пусто,
        кк_виконання_шаблону = пусто
      },
      дані_секції,
      місцезнаходження
    );

    якщо результат.помилка != пусто {
      вернути результат;
    }

    змінити_в_середовищі(контекст.середовище, назва_зовнішня(назва_т8), Обʼєкт { ВидОбʼєктаСекція, результат.обʼєкт_секції });

    вернути результат;
  }
}