взяти визначення ./КЦ;

секція КЦ {
  місцева дія створити_текст_коду(компілятор: адреса<Компілятор>) -> адреса<ТекстКоду> {
    змінна текст_коду = виділити<ТекстКоду>(компілятор);

    текст_коду.наступний = пусто;
    текст_коду.компілятор = компілятор;
    текст_коду.шлях = Шлях { 0, пусто };
    текст_коду.значення = ю8 { 0, пусто };

    якщо компілятор.перший_текст_коду == пусто {
      компілятор.перший_текст_коду = текст_коду;
    }
    якщо компілятор.останній_текст_коду == пусто {
      компілятор.останній_текст_коду = текст_коду;
    } інакше {
      компілятор.останній_текст_коду.наступний = текст_коду;
      компілятор.останній_текст_коду = текст_коду;
    }

    вернути текст_коду;
  }

  місцева дія знищити_текст_коду(текст_коду: адреса<ТекстКоду>) {
    змінна компілятор = текст_коду.компілятор;

    знищити_шлях(компілятор, текст_коду.шлях);

    звільнити_памʼять(компілятор, текст_коду.значення.дані);

    звільнити(компілятор, текст_коду);
  }
}