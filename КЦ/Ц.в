    реєстр_готових_значень_звичайного_шаблону: адреса<Реєстр<Накопичувач<адреса<ОбʼєктТипу>>, Обʼєкт>>;

    місцезнаходження_асд_даних_шаблону_структури: адреса<Місцезнаходження>;
    асд_дані_шаблону_структури: адреса<АСДДаніСтруктура>;
    реєстр_готових_значень_шаблону_структури: адреса<Реєстр<Накопичувач<адреса<ОбʼєктТипу>>, адреса<ОбʼєктТипу>>>;

    місцезнаходження_асд_даних_шаблону_дії: адреса<Місцезнаходження>;
    асд_дані_шаблону_дії: адреса<АСДДаніДія>;
    реєстр_готових_значень_шаблону_дії: адреса<Реєстр<Накопичувач<адреса<ОбʼєктТипу>>, адреса<ОбʼєктДії>>>;

    список_асд_значень_шаблону_групи_дій: Накопичувач<адреса<АСДЗначенняДії>>;
    реєстр_готових_значень_шаблону_групи_дій: адреса<Реєстр<Накопичувач<адреса<ОбʼєктТипу>>, адреса<ОбʼєктГрупиДій>>>;
  }

  структура ІнформаціяВиконанняШаблону {
    шаблон: адреса<ОбʼєктШаблону>;
    аргументи: Накопичувач<адреса<ОбʼєктТипу>>;
  }

