взяти визначення різне;
взяти визначення розбирач;
взяти визначення LLVM;
взяти визначення біб;
взяти визначення накопичувач;
взяти визначення реєстр;

зовнішня дія зʼєднати_ю8(значення: памʼять<п8>, значення2: памʼять<п8>): памʼять<п8>;

зовнішня дія strcmp(а: памʼять<п8>, б: памʼять<п8>): ц32;

синонім буфер_ю8 = Накопичувач<п8>;

дія створити_буфер_ю8(): адреса<Накопичувач<п8>> {
  вернути виділити_накопичувач<п8>();
}

дія додати_до_буфера_ю8(буфер: адреса<Накопичувач<п8>>, значення: памʼять<п8>) {
  змінна ціль х: позитивне = 0;
  поки значення[х] != 0 {
    покласти_в_накопичувач<п8>(буфер, значення[х]);
    х = х + 1;
  }
}

дія видалити_останнє_значення_з_буфера_ю8(буфер: адреса<Накопичувач<п8>>) {
  буфер.розмір = буфер.розмір - 1;
}

дія додати_символ_до_буфера_ю8(буфер: адреса<Накопичувач<п8>>, значення: п8) {
  покласти_в_накопичувач<п8>(буфер, значення);
}

дія звільнити_буфер_ю8(буфер: адреса<Накопичувач<п8>>) {
  звільнити_накопичувач<п8>(буфер);
}

дія створити_ю8_сі_з_буфера(буфер: адреса<Накопичувач<п8>>): памʼять<п8> {
  ціль значення = виділити<п8>((буфер.розмір + 1));
  змінна ціль х: позитивне = 0;
  поки х < буфер.розмір {
    значення[х] = буфер.дані[х];
    х = х + 1;
  }
  значення[буфер.розмір] = 0;
  вернути значення;
}

дія створити_зворотний_ю8_сі_з_буфера(буфер: адреса<Накопичувач<п8>>): памʼять<п8> {
  ціль значення = виділити<п8>((буфер.розмір + 1));
  змінна ціль х = буфер.розмір;
  поки х > 0 {
    значення[буфер.розмір - х] = буфер.дані[х - 1];
    х = х - 1;
  }
  значення[буфер.розмір] = 0;
  вернути значення;
}

дія ЯТУТ() {
  біб::друк_ю8(ю8"Я ТУТ!");
}

дія ЯТУТ_0() {
  біб::друк_ю8(ю8"Я ТУТ 0!");
}

дія ЯТУТ_1() {
  біб::друк_ю8(ю8"Я ТУТ 1!");
}

