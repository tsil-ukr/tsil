    тип: адреса<ОбʼєктТипу>;
  }

  структура ОбʼєктТипу {
    вид: позитивне;
    назва: памʼять<п8>;
    властивості_структури: Накопичувач<ВластивістьСтруктури>;
    параметри_дії: Накопичувач<ПараметрДії>;
    тип_результату_дії: адреса<ОбʼєктТипу>;
    llvm_function_type: адреса<LLVM::FunctionType>;
    тип_адреси: адреса<ОбʼєктТипу>;
    тип_памʼяті: адреса<ОбʼєктТипу>;
    тип_масиву: адреса<ОбʼєктТипу>;
    розмір_масиву: позитивне;
    типи_варіації: Накопичувач<адреса<ОбʼєктТипу>>;
    найбільший_тип_варіації: адреса<ОбʼєктТипу>;
    елементи_переліку: Накопичувач<адреса<ОбʼєктТипу>>;
    обʼєкт_типу_переліку_елемента_переліку: адреса<ОбʼєктТипу>;
    позиція_елемента_переліку: позитивне;
    обʼєкт_типу_структури_елемента_переліку: адреса<ОбʼєктТипу>;
    найбільший_тип_переліку: адреса<ОбʼєктТипу>;
    типи_шаблону: Накопичувач<адреса<ОбʼєктТипу>>;
    llvm_type: адреса<LLVM::Type>;
  }
  місцева дія виділити_обʼєкт_типу(вид: позитивне): адреса<ОбʼєктТипу>;
  місцева дія виділити_нативний_тип(К: адреса<Компілятор>, назва: памʼять<п8>, llvm_type: адреса<LLVM::Type>): адреса<ОбʼєктТипу>;
