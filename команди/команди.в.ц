взяти біб цілеспрямовано;

місцева дія ціль_допомога(виділяч: адреса<біб::Виділяч>,
                          опції_програми: цілеспрямовано::Опції<ю8>,
                          опції_команди: цілеспрямовано::Опції<ю8>) -> ц32;

місцева дія ціль_версія(виділяч: адреса<біб::Виділяч>,
                        опції_програми: цілеспрямовано::Опції<ю8>,
                        опції_команди: цілеспрямовано::Опції<ю8>) -> ц32;

місцева дія ціль_скомпілювати(виділяч: адреса<біб::Виділяч>,
                              опції_програми: цілеспрямовано::Опції<ю8>,
                              опції_команди: цілеспрямовано::Опції<ю8>,
                              вхід: ю8,
                              опції_входу: цілеспрямовано::Опції<ю8>,
                              вихід: ю8,
                              опції_виходу: цілеспрямовано::Опції<ю8>,
                              треба_знищити_залишки: логічне) -> ц32;

місцева дія ціль_ювкд(виділяч: адреса<біб::Виділяч>,
                      опції_програми: цілеспрямовано::Опції<ю8>,
                      опції_команди: цілеспрямовано::Опції<ю8>,
                      вхід: ю8,
                      опції_входу: цілеспрямовано::Опції<ю8>,
                      вихід: ю8,
                      опції_виходу: цілеспрямовано::Опції<ю8>,
                      треба_знищити_залишки: логічне) -> ц32;

місцева дія ціль_кдвю(виділяч: адреса<біб::Виділяч>,
                      опції_програми: цілеспрямовано::Опції<ю8>,
                      опції_команди: цілеспрямовано::Опції<ю8>,
                      вхід: ю8,
                      опції_входу: цілеспрямовано::Опції<ю8>,
                      вихід: ю8,
                      опції_виходу: цілеспрямовано::Опції<ю8>,
                      треба_знищити_залишки: логічне) -> ц32;