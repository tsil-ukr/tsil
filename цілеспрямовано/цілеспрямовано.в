взяти визначення біб/Виділяч;

секція цілеспрямовано {
  дія т8_рівні(а: т8, б: т8): логічне {
    якщо а.розмір != б.розмір {
      вернути ні;
    }

    змінна п: позитивне = 0;

    поки п < а.розмір {
      якщо а.дані[п] != б.дані[п] {
        вернути ні;
      }
      п += 1;
    }

    вернути так;
  }

  структура можливо<Т> {
    заповнено: позитивне;
    значення: Т;
  }

  структура РозібранаОпція {
    ключ: т8;
    значення: можливо<т8>;
  }

  структура РозібраніОпції {
    розмір: позитивне;
    дані: памʼять<РозібранаОпція>;
  }

  структура РозібраніВиходиКоманди {
    розмір: позитивне;
    дані: памʼять<т8>;
  }

  структура РозібраніВходиКоманди {
    розмір: позитивне;
    дані: памʼять<т8>;
  }

  структура РозібранаКоманда {
    опції_програми: РозібраніОпції;
    виходи: РозібраніВиходиКоманди;
    команда: т8;
    опції_команди: РозібраніОпції;
    входи: РозібраніВходиКоманди;
  }

  структура АргументиДляРозбору {
    розмір: позитивне;
    дані: памʼять<т8>;
  }

  структура ДоступніКомандиДляРозбору {
    розмір: позитивне;
    дані: памʼять<т8>;
  }

  дія розібрати_команду(аргументи: АргументиДляРозбору,
                        доступні_команди: ДоступніКомандиДляРозбору,
                        вихід: адреса<РозібранаКоманда>): логічне {
    якщо аргументи.розмір == 0 {
      вернути ні;
    }


  }
}