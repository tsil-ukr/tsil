структура т8 { і8, адреса }

константа @значення = ц8[6] { 'п', 'р', 'и', 'в', 'і', 'т' }
глобальна @значення = ц8[6] { 'п', 'р', 'и', 'в', 'і', 'т' }

інструкція @друк(%значення т8) -> ц32 {
крок накопичення:
  %аа = накопичити ц32
