структура т8 { ц8, адреса }

внутрішня глобальна @"якісь_дані" ц8[6] { 'п', 'р', 'и', 'в', 'і', 'т' }

місцева інструкція @"друк" (%значення т8) -> ц32 {
накопичення:
  %аа = накопичити ц32
початок:
  записати ц32 123 в %аа
  стрибнути на кінець
кінець:
  %а = прочитати ц32 з %аа
  виконати @"друк" (т8 { ц8 0, адреса 0 })
  %д = додати ц32 123, ц32 321
  вернути %а
}