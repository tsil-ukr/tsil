структура ЛМодуль;
структура ЛТип;
структура ЛФункція;
структура ЛТипФункції;
структура ЛЗначення;
структура ЛБлок;

зовнішня дія xlm_create(назва: памʼять<п8>): комірка<ЛМодуль>;
зовнішня дія dumpLL(Л: комірка<ЛМодуль>): памʼять<п8>;

зовнішня дія xl_create_struct(Л: комірка<ЛМодуль>, назва: памʼять<п8>): комірка<ЛТип>;
зовнішня дія xl_set_struct_fields(Л: комірка<ЛМодуль>, с: комірка<ЛТип>, розмір_полів: позитивне, поля: памʼять<комірка<ЛТип>>);

зовнішня дія xl_declare_function(Л: комірка<ЛМодуль>, назва: памʼять<п8>, тип_повернення: комірка<ЛТип>, розмір_параметрів: позитивне, параметри: памʼять<комірка<ЛТип>>, варарг: позитивне): комірка<ЛТипФункції>;

зовнішня дія xl_get_null(Л: комірка<ЛМодуль>): комірка<ЛЗначення>;
зовнішня дія xl_get_void_type(Л: комірка<ЛМодуль>): комірка<ЛТип>;

секція лл {
  дія створити(назва: памʼять<п8>): комірка<ЛМодуль> {
    вернути xlm_create(назва);
  }

  дія відмалювати(Л: комірка<ЛМодуль>): памʼять<п8> {
    вернути dumpLL(Л);
  }

  дія створити_структуру(Л: комірка<ЛМодуль>, назва: памʼять<п8>): комірка<ЛТип> {
    вернути xl_create_struct(Л, назва);
  }

  дія створити_дію(Л: комірка<ЛМодуль>, назва: памʼять<п8>, тип_повернення: комірка<ЛТип>, розмір_параметрів: позитивне, параметри: памʼять<комірка<ЛТип>>, варарг: позитивне): комірка<ЛТипФункції> {
    вернути xl_declare_function(Л, назва, тип_повернення, розмір_параметрів, параметри, варарг);
  }

  дія отримати_значення_пусто(Л: комірка<ЛМодуль>): комірка<ЛЗначення> {
    вернути xl_get_null(Л);
  }

  дія отримати_тип_ніщо(Л: комірка<ЛМодуль>): комірка<ЛТип> {
    вернути xl_get_void_type(Л);
  }
}