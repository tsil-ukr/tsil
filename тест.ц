структура S<T>;
структура S<T> {
  a: T;
}

дія alloc<Т>(count: ц32): памʼять<Т>;
дія alloc<Т>(): памʼять<Т>;

//дія alloc<Т>(count: ц32): памʼять<Т> {
//}

дія alloc<Т>(count: ц32): памʼять<Т> {
}

дія alloc<Т>(): памʼять<Т> {
}

//дія виділити<Т>(): комірка<Т> {
