структура Some;

структура Some {
  a: ц16;
  b: ц16;
}

структура Coords {
  x: ц32;
  y: ц32;
  z: Some;
}

//дія x(): ц32 {}

//дія t(a1: ц32, a2: ц8);

дія main() {
  ціль v1: ц32;
  ціль v2: ц32;
  v1 = v2;
  ціль v3: Coords;
  v3.x = v1;
//  ціль v4 = x();
  ціль v5 = v3.y;
  v3.z.a = v3.z.b;
//  ціль v6: комірка<Some> = пусто;
//  ціль v7 = v6.a;
}
