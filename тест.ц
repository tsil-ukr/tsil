зовнішня дія main() {
  alloc<ц32>(1);
  alloc<ц8>(1);
  alloc<S<ц8>>(1);
  ціль s: S<ц8>;
  ціль s1: S<ц8>;
  ціль s2: S<ц32>;
  ціль s3: S<ц64>;
}