структура A {
  x: ц8;
}

структура B {
  a: A;
}

структура C {
  b: B;
}

зовнішня дія putchar(char: ц8): ц32;

дія test() {
  ціль c: C;
  ціль m = c.b.a.x;
  ціль b = 2;
  c.b.a.x = 90;
  b = 10;
  putchar(c.b.a.x);
  ціль aaa: A = { 90 };
  putchar(aaa.x);
}

зовнішня дія main(): ц32 {
  test();
}

