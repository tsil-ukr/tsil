зовнішня дія malloc(розмір: ц32): невідома_памʼять;
зовнішня дія putchar(значення: ц8): ц32;

структура Test {
  a: ц8;
  b: ц8;
  c: ц8;
  d: ц32;
}

структура X;

зовнішня дія main(): ц32 {
  ціль test = malloc(/* Test.розмір */ 8) як комірка<Test>;
  test.a = 1;
  test.b = 2;
  test.c = 3;
  test.d = 4;
  ціль xx = X.розмір;
}