  дія отримати_ір(модуль: адреса<Модуль>, вихід_розміру: адреса<натуральне>, вихід_даних: адреса<памʼять<н8>>) -> логічне {
    вернути __ЛЛВМ__отримати_ір(модуль, вихід_розміру, вихід_даних);
  }
}