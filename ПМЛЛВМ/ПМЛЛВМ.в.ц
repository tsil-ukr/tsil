взяти визначення ПМ;

секція ПМЛЛВМ {
  місцева дія отримати_лл(модуль: адреса<ПМ::Модуль>, вихід_розміру: адреса<натуральне>, вихід_даних: адреса<памʼять<н8>>) -> логічне;

  місцева дія отримати_обджект(модуль: адреса<ПМ::Модуль>, вихід_розміру: адреса<натуральне>, вихід_даних: адреса<памʼять<н8>>) -> логічне;
}